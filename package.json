    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.1.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unicorn": "^47.0.0"
  },
  "peerDependencies": {
    "eslint": ">=8 <9"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "mocha": "^10.1.0"
  }
